	  	<servlet-class>com.somnath.digital.user.servlets.EditUserServlet</servlet-class>
	  </servlet>
	  <servlet>
	  	<servlet-name>Login</servlet-name>
	  	<display-name>Login</display-name>
	  	<servlet-class>com.somnath.digital.user.servlets.Login</servlet-class>
	  </servlet>
	  
	  <servlet>
	  	<servlet-name>Logout</servlet-name>
	  	<display-name>Logout</display-name>
	  	
	  	<servlet-class>com.somnath.digital.user.servlets.Logout</servlet-class>
	  </servlet>
	  <servlet>
	  	<servlet-name>ForgetPasswordServlet</servlet-name>
