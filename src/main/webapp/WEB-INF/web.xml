<web-app>

	  <display-name> Archetype Created Web web</display-name>
	  
	  <servlet>
	  	<servlet-name>CreateUserServlet</servlet-name>
	  	<servlet-class>com.somnath.digital.user.servlets.CreateUserServlet</servlet-class>
	  </servlet>
	  <servlet>
	  	<servlet-name>DeleteUserServlet</servlet-name>
	  	<display-name>DeleteUserServlet</display-name>
	  	<description></description>
	  	<servlet-class>com.somnath.digital.user.servlets.DeleteUserServlet</servlet-class>
	  </servlet>
	  
	  <servlet>
	  	<display-name>EditUserServlet</display-name>
